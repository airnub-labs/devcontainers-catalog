{
  "name": "@airnub/devc",
  "version": "0.1.0",
  "description": "Agnostic Dev Container generator for presets, lesson scaffolds, and aggregate compose.",
  "bin": {
    "devc": "dist/index.js"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "license": "Apache-2.0",
  "repository": "https://github.com/airnub-labs/devcontainers-catalog",
  "type": "module",
  "exports": {
    ".": "./dist/sdk.js",
    "./cli": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx src/index.ts",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "docs:json": "typedoc --json ../../docs-site/api/airnub-devc.json"
  },
  "dependencies": {
    "@airnub/sdk-codespaces-adapter": "file:../../packages/sdk-codespaces-adapter",
    "@clack/prompts": "^0.7.0",
    "@types/node": "^20.12.12",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "deepmerge": "^4.3.1",
    "execa": "^9.3.0",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.2",
    "tar": "^7.4.3",
    "yaml": "^2.6.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^9.10.0",
    "typedoc": "^0.26.0",
    "typedoc-plugin-markdown": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^1.6.0"
  }
}
